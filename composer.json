{
    "name": "hopstudios/hop-reveal",
    "description": "Hop Reveal",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "Hop Reveal"
    ],
    "support": {
        "docs": "https://github.com/hop-studios/hop-reveal/blob/master/README.md",
        "issues": "https://github.com/hop-studios/hop-reveal/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hop Studios",
            "homepage": "https://hopstudios.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "hopstudios\\hopreveal\\": "src/"
        }
    },
    "extra": {
        "name": "Hop Reveal",
        "handle": "hop-reveal",
        "developer": "Hop Studios",
        "developerUrl": "hopstudios.com",
        "documentationUrl": "https://github.com/hopstudios/hop-reveal/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/hopstudios/hop-reveal/master/CHANGELOG.md",
        "components": {
            "hopConfigHelperService": "hopstudios\\hopreveal\\services\\HopRevealService"
        },
        "class": "hopstudios\\hopreveal\\HopReveal"
    }
}
